{
  "name": "netwrix-docs",
  "version": "0.1",
  "private": true,
  "main": "docusaurus.config.js",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "node scripts/start-wrapper.js",
    "start:all": "docusaurus start",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 npx docusaurus build",
    "build-render": "npm install @rspack/binding-linux-x64-gnu; npm install; npx docusaurus build",
    "build:single": "node scripts/build-single.js",
    "swizzle": "docusaurus swizzle",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "update-mappings": "node scripts/generate-product-mappings.js"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@docusaurus/babel": "^3.8.1",
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/faster": "^3.8.1",
    "@docusaurus/plugin-google-gtag": "^3.8.1",
    "@docusaurus/plugin-google-tag-manager": "^3.8.1",
    "@docusaurus/plugin-rsdoctor": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@mdx-js/react": "^3.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-typed": "^2.0.12"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@mdx-js/mdx": "^3.1.0",
    "husky": "^9.1.7",
    "ora": "^8.2.0",
    "prettier": "3.5.3",
    "table": "^6.9.0",
    "to-vfile": "^8.0.0",
    "vfile-reporter": "^8.1.1"
  },
  "engines": {
    "node": ">=18.0"
  }
}
